{
  "name": "css-ngapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format:prettier:check": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"src/**/*.{js,json,css}\" -l",
    "format:prettier:fix": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"src/**/*.{js,json,css}\" --write",
    "format:html:check": "node bin/glob-ls.js 'src/**/*.html'  | xargs  ./bin/html-beautify-check.sh",
    "format:html:fix": "node bin/glob-ls.js 'src/**/*.html'  | xargs ./node_modules/.bin/html-beautify -r",
    "format:check": "npm run format:prettier:check && npm run format:html:check",
    "format:fix": "npm run format:prettier:fix && npm run format:html:fix",
    "lint:fix": "ng lint --fix",
    "format:lint:fix": "npm run format:fix && npm run lint:fix",
    "compodoc:gen": "./node_modules/.bin/compodoc -p ./tsconfig.json",
    "compodoc:serve": "./node_modules/.bin/compodoc -s ./tsconfig.json",
    "compodoc": "npm run compodoc:gen && npm run compodoc:serve",
    "build:prod": "ng build --prod",
    "build:prod-mr": "ng build --configuration=production-mr",
    "build:prod-fr": "ng build --configuration=production-fr",
    "build:qa": "ng build --configuration=qa",
    "build:qa-mr": "ng build --configuration=qa-mr",
    "build:qa-fr": "ng build --configuration=qa-fr",
    "build:alllangs:prod": "npm run build:prod && npm run build:prod-mr && npm run build:prod-fr",
    "build:alllangs:qa": "npm run build:qa && npm run build:qa-mr && npm run build:qa-fr",
    "extract-i18n": "ng xi18n css-ngapp --i18n-format xlf --output-path src/i18n --i18n-locale en && ngx-extractor -i src/**/*.ts -o src/i18n/messages.xlf && ng run css-ngapp:xliffmerge",
    "start-prod": "ng serve --configuration=production --port 5300",
    "start-prod-mr": "ng serve --configuration=production --port 5301",
    "start-prod-fr": "ng serve --configuration=production --port 5302",
    "start-qa": "ng serve --configuration=qa --port 6300",
    "start-qa-mr": "ng serve --configuration=qa --port 6301",
    "start-qa-fr": "ng serve --configuration=qa --port 6302",
    "start-mr": "ng serve --configuration=mr --port 4201",
    "start-fr": "ng serve --configuration=fr --port 4202"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "ngx-logger": "^4.0.8",
    "popper.js": "^1.16.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "^8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@compodoc/compodoc": "^1.1.10",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.1",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.10.2",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:check && npm run lint"
    }
  }
}
